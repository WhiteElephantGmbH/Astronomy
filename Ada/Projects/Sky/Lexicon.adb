pragma Style_White_Elephant;

with Ada.Containers.Indefinite_Ordered_Maps;
with Language;
with Traces;

package body Lexicon is

  package Log is new Traces ("Lexicon");

  type Name_Entry is array (Language.Kind) of access constant String;

  type Name_Table is array (Word) of Name_Entry;

  package Words is new Ada.Containers.Indefinite_Ordered_Maps (Key_Type => String, Element_Type => Word);

  type Word_Mapping is array (Language.Kind) of Words.Map;

  The_Word_Mapping : Word_Mapping;


  function E (The_Key : Word;
              English : String;
              French  : String;
              German  : String;
              Greek   : String;
              Italian : String;
              Spanish : String) return Name_Entry is

    procedure Insert (Item : String;
                      Kind : Language.Kind) is
    begin
      The_Word_Mapping(Kind).Insert (Item, The_Key);
    exception
    when others =>
      Log.Error ("Defined twice: " & Item & " for " & Kind'image);
    end Insert;

  begin -- E
    Insert (English, Language.English);
    Insert (French, Language.French);
    Insert (German, Language.German);
    Insert (Greek, Language.Greek);
    Insert (Italian, Language.Italian);
    Insert (Spanish, Language.Spanish);
    return [Language.English => new String'(English),
            Language.French  => new String'(French),
            Language.German  => new String'(German),
            Language.Greek   => new String'(Greek),
            Language.Italian => new String'(Italian),
            Language.Spanish => new String'(Spanish)];
  end E;

