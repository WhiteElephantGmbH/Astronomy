  function Image_Of (Item : Word) return String is
  begin
    return Dictionary(Item)(Language.Actual).all;
  end Image_Of;


  function Word_Of (Name : String) return Word is
    use type Language.Kind;
  begin
    begin
      return The_Word_Mapping(Language.Actual).Element (Name);
    exception
    when others =>
      null;
    end;
    for The_Language in Language.Kind loop
      if The_Language /= Language.Actual then
        begin
          return The_Word_Mapping(The_Language).Element (Name);
        exception
        when others =>
          null;
        end;
      end if;
    end loop;
    raise Not_Found;
  end Word_Of;

